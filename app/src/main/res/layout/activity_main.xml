        android:background="#d02918"
        android:orientation="vertical"
        android:layout_marginBottom="1dp"
        android:elevation="5dp">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:src="@drawable/actionbar"/>


    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#2e2e2e">
        <TextView
            android:id="@+id/intersial"
            android:textColor="#fff"
            android:paddingLeft="25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Interstitial"
            android:layout_alignParentLeft="true"
            android:paddingTop="7dp"
            android:paddingBottom="7dp"/>
        <TextView
            android:id="@+id/toload"
            android:background="#5a595e"
            android:paddingTop="7dp"
            android:paddingBottom="7dp"
            android:paddingLeft="30dp"
            android:paddingRight="27dp"
            android:textColor="#fff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="LOAD BANNER"/>
        <TextView
            android:id="@+id/toMarket"
            android:textColor="#fff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="BANNER : 9"
            android:paddingRight="15dp"
            android:layout_alignParentRight="true"
            android:paddingTop="7dp"
            android:paddingBottom="7dp"/>
    </RelativeLayout>

    <RelativeLayout
